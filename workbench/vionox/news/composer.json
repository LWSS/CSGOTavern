{
	"name": "vionox/news",
	"type": "platform-extension",
	"description": "Website News Handling",
	"authors": [
		{
			"name": "Brandon Xversial",
			"email": "Xversial@vionox.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "~5.0",
		"platform/foundation": "2.0.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Vionox\\News\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
